!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title AlexisRabago
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body{ "ng-app" => "resume", "ng-controller" => "navigationController" }
    .sidebar.pull-left
      .thumbnail
        = image_tag "profile.jpg", class: "img-circle profile-image"
        .caption
          %ul.menu
            %li.menu-link{ "ng-repeat" => "menu in menus", "ng-mouseover" => "hoverIn()", "ng-class" => "hoverMenu", "ng-mouseout" => "hoverOut()" }
              %a{ href: "#", class: "text-center", "ng-click" => "navigateTo(menu)"}
                {{menu}}
    = yield
    .clearfix
