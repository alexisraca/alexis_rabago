!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title AlexisRabago
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body{ "ng-app" => "resume", "ng-controller" => "navigationController" }
    .sidebar.pull-left
      =link_to  "jobs", "#", "ng-click" => "navigateTo('jobs')"
      =link_to "skills", "#", "ng-click" => "navigateTo('skills')"
    = yield
    .clearfix
